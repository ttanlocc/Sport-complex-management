                                    Height="64"
                                    Margin="156,145,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Content="">
                                    <Label.Background>
                                        <ImageBrush ImageSource="image/dollar.png" Stretch="Uniform" />
                                    </Label.Background>
                                </Label>
                                <Label
                                    x:Name="label_Copy4"
