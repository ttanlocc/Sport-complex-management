                                    x:Name="usd"
                                    Width="344"
                                    Height="33"
                                    Margin="29,104,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    HorizontalContentAlignment="Center"
                                    Content="{Binding AlltimerevenueUSD}"
                                    FontSize="22"
                                    FontWeight="Bold"
                                    Foreground="#DDF9F9F9"
                                    RenderTransformOrigin="0.5,0.5">
                                    <Label.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform />
                                            <SkewTransform />
                                            <RotateTransform Angle="-0.549" />
                                            <TranslateTransform />
                                        </TransformGroup>
                                    </Label.RenderTransform>
                                </Label>
                            </Grid>
