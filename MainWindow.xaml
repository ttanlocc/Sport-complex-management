                                Width="30"
                                Height="30"
                                HorizontalAlignment="Center"
                                Kind="ViewDashboard" />
                            <TextBlock HorizontalAlignment="Center" Text="Main" />
                        </StackPanel>
                    </TabItem.Header>
                    <!--  MainUI start here  -->
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="250" />
                            <RowDefinition Height="*" />

                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="400" />
                                <ColumnDefinition Width="400" />
                                <ColumnDefinition Width="400" />
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0">
                                <materialDesign:Card
                                    Margin="25"
                                    Padding="8"
                                    materialDesign:ShadowAssist.ShadowDepth="Depth5"
                                    Foreground="{x:Null}">

                                    <materialDesign:Card.Background>
                                        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                            <GradientStop Offset="1" Color="#FF7D27FF" />
                                            <GradientStop Color="#FFBB2086" />
                                        </LinearGradientBrush>
                                    </materialDesign:Card.Background>

                                </materialDesign:Card>
                                <Label
                                    x:Name="label_money"
                                    Width="93"
                                    Height="64"
                                    Margin="150,147,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Content="">
                                    <Label.Background>
                                        <ImageBrush ImageSource="image/money.png" Stretch="Uniform" />
                                    </Label.Background>
                                </Label>
                                <Label
                                    x:Name="label_income"
                                    Width="346"
                                    Height="39"
                                    Margin="26,39,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    HorizontalContentAlignment="Center"
                                    Content="ALL INCOME IN VND"
                                    FontSize="24"
                                    FontWeight="Bold"
                                    Foreground="#DDF9F9F9" />
                                <Label
                                    x:Name="income"
                                    Width="344"
                                    Height="31"
                                    Margin="28,105,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    HorizontalContentAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    Content="{Binding Alltimerevenue}"
                                    FontSize="22"
                                    FontWeight="Bold"
                                    Foreground="#DDF9F9F9" />
                            </Grid>
                            <Grid Grid.Column="1">
                                <materialDesign:Card
                                    Margin="25"
                                    Padding="8"
                                    materialDesign:ShadowAssist.ShadowDepth="Depth5"
                                    Foreground="{x:Null}">

                                    <materialDesign:Card.Background>
                                        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                            <GradientStop Color="#FFF51C40" />
                                            <GradientStop Offset="1" Color="#FFD89B65" />
                                        </LinearGradientBrush>
                                    </materialDesign:Card.Background>

                                </materialDesign:Card>
                                <Label
                                    x:Name="label_guesttext"
                                    Width="347"
                                    Height="39"
                                    Margin="27,39,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    HorizontalContentAlignment="Center"
                                    Content="THIS MONTH"
                                    FontSize="24"
                                    FontWeight="Bold"
                                    Foreground="#DDF9F9F9" />
                                <Label
                                    x:Name="label_guest"
                                    Width="93"
                                    Height="64"
                                    Margin="147,149,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Content="">
                                    <Label.Background>
                                        <ImageBrush ImageSource="image/tags.png" Stretch="Uniform" />
                                    </Label.Background>
                                </Label>
                                <Label
                                    x:Name="guest"
                                    Width="344"
                                    Height="33"
                                    Margin="29,104,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    HorizontalContentAlignment="Center"
                                    Content="{Binding ThisMonthRevenue}"
                                    FontSize="22"
                                    FontWeight="Bold"
                                    Foreground="#DDF9F9F9" />
                            </Grid>
                            <Grid Grid.Column="2">
                                <materialDesign:Card
                                    Margin="25"
                                    Padding="8"
                                    materialDesign:ShadowAssist.ShadowDepth="Depth5"
                                    Foreground="{x:Null}">

                                    <materialDesign:Card.Background>
                                        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                            <GradientStop Offset="1" Color="#FF016CFD" />
                                            <GradientStop Color="#FF00E4FF" />
                                        </LinearGradientBrush>
                                    </materialDesign:Card.Background>

                                </materialDesign:Card>
