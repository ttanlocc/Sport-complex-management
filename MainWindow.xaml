                                <Grid Grid.Row="2" Margin="10,20,10,0">

                                    <TextBlock
                                        Margin="20,5,10,0"
                                        FontSize="24"
                                        FontWeight="Bold"
                                        Text="Total"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        x:Name="total"
                                        Margin="20,5,10,0"
